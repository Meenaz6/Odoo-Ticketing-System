<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="assets_backend" name="Ticket Assets Backend" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/Ticket/static/src/js/dashboard.js"/>
                <link rel="stylesheet" type="text/css" href="/Ticket/static/src/css/dashboard.css"/>
            </xpath>
        </template>

        <template id="ticket_assets_qweb" inherit_id="web.assets_qweb">
            <xpath expr="." position="inside">
                <script type="text/qweb" src="/Ticket/static/src/xml/dashboard_template.xml"/>
            </xpath>
        </template>
    </data>
</odoo>